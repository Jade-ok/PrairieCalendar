{
  "manifest_version": 3,
  "name": "PrairieCalendar",
  "version": "2.0",

  "action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "assets/icon master16.png",
      "48": "assets/icon master48.png",
      "128": "assets/icon master128.png"
    }
  },

  "icons": {
    "16": "assets/icon master16.png",
    "48": "assets/icon master48.png",
    "128": "assets/icon master128.png"
  },
  "permissions": ["storage", "identity"],
  "host_permissions": ["https://www.googleapis.com/*"],
  "oauth2": {
    "client_id": "958094905068-vahp82rrncofr53edkavo9jpfqc39e6m.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/calendar.events"]
  },

  "content_scripts": [
    {
      "matches": ["https://us.prairietest.com/*"],
      "js": ["src/content.js"]
    }
  ]
}
